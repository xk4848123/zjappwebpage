<?xml version='1.0' encoding='utf-8'?>
<widget id="com.zhongjian.technologycal.mall" version="2.0.13" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
